config_version=4

[application]

config/name="where the flood couldn't reach"
config/description="Some place quiet to rest your head."
run/main_scene="res://Components/Title.tscn"
config/icon="res://Components/Textures/isopodRadiodontBait.png"
config/macos_native_icon="res://Art/isopodRadiodontBait.icns"
config/windows_native_icon="res://Art/isopodRadiodontBait.ico"

[autoload]

UniversalFunctions="*res://Source/UniversalFunctions.gd"
DataStorage="*res://Source/DataStorage.gd"

[display]

window/size/width=300
window/size/height=200
window/size/test_width=1200
window/size/test_height=800
window/stretch/mode="2d"
window/stretch/aspect="keep"

[input]

ui_end={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":16777230,"physical_scancode":0,"unicode":0,"echo":false,"script":null)
 ]
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

2d/snapping/use_gpu_pixel_snap=true
environment/default_environment="res://default_env.tres"
